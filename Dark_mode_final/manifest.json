{
    "name":"Dark Mode by Abbas",
    "version":"1.0.0.0",
    "description":"This extension changes the theme of google classroom to dark.",
    "manifest_version":3,
    "action":{
        "default_icon":"icon.png",
        "default_popup":"index.html"
    },
    "content_scripts":[
        {
            "matches":[
                "https://*.classroom.google.com/*"
            ],
            "js":[
                "app.js",
                "script.js"
            ]
        }
    ]
}
